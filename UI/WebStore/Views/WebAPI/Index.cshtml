@model IEnumerable<string>
@{
    ViewData["Title"] = "Тест WebAPI";
}
@section Styles {
    <partial name="Partial/_Index_Styles" />
}
@section Scripts {
    <partial name="Partial/_Index_Scripts" />
}

<div class="single_post">
    <div class="container">
        <div class="row">
            <div class="col-lg-8 offset-lg-2">
                <div class="single_post_title">@ViewData["Title"]</div>
                <div class="single_post_text">
                    <table class="table">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>value</th>
                            </tr>
                        </thead>
                        <tbody>
                            @{ var i = 1; }
                            @foreach (var value in Model)
                            {
                                <tr>
                                    <td>@(i++)</td>
                                    <td>@value</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
